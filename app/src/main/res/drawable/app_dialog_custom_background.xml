<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item

        android:bottom="1dp"
        android:top="1dp"
        android:right="1dp"
        android:left="1dp">

        <shape
            android:shape="rectangle">

            <corners
                android:radius="20dp" />

            <stroke
                android:color="#000000"
                android:width="2dp" />

            <solid
                android:color="@color/app_Bar_background"/>
        </shape>
    </item>
</layer-list>