<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Labels">
        <item name="android:alpha">1.0</item>
        <item name="android:fontFamily">@font/open_sans</item>
    </style>

    <style name="appBrand">
        <item name="android:alpha">1.0</item>
        <item name="android:fontFamily">@font/open_sans_light</item>
    </style>

    <style name="appBrand.Heading">
        <item name="android:textSize">@dimen/labels_heading</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="appBrand.Heading.Light">
        <item name="android:textColor">@color/colorTextPrimary</item>
    </style>
    <style name="appBrand.Heading.Dark">
        <item name="android:textColor">@color/colorTextSecondary</item>
    </style>

    <style name="Labels.Toolbar">
        <item name="android:fontFamily">@font/open_sans_light</item>
        <item name="android:textSize">@dimen/labels_appBar_text</item>
    </style>

    <style name="Labels.Toolbar.Light">
        <item name="android:textColor">@color/colorTextPrimary</item>
    </style>

    <style name="Labels.Toolbar.Dark">
        <item name="android:textColor">@color/colorTextSecondary</item>
    </style>

    <style name="Labels.Heading">
        <item name="android:textSize">@dimen/labels_heading</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Labels.Heading.Light">
        <item name="android:textColor">@color/colorTextPrimary</item>
    </style>

    <style name="Labels.Heading.Dark">
        <item name="android:textColor">@color/colorTextSecondary</item>
    </style>

    <style name="Labels.Title">
        <item name="android:textSize">@dimen/labels_titles</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Labels.Title.Light">
        <item name="android:textColor">@color/colorTextPrimary</item>
    </style>

    <style name="Labels.Title.Dark">
        <item name="android:textColor">@color/colorTextSecondary</item>
    </style>


    <style name="Labels.Emphasis">
        <item name="android:fontFamily">@font/open_sans_light</item>
        <item name="android:textSize">@dimen/labels_emphasis</item>

    </style>

    <style name="Labels.Emphasis.Light">
        <item name="android:textColor">@color/colorTextPrimary</item>

    </style>

    <style name="Labels.Emphasis.Dark">
        <item name="android:textColor">@color/colorTextSecondary</item>

    </style>

    <style name="Labels.Body">
        <item name="android:fontFamily">@font/open_sans</item>
    </style>

    <style name="Labels.Body.Normal">
        <item name="android:textSize">@dimen/labels_body_normal</item>
    </style>

    <style name="Labels.Body.Normal.Light">
        <item name="android:textColor">@color/colorTextPrimary</item>
    </style>

    <style name="Labels.Body.Normal.Dark">
        <item name="android:textColor">@color/colorTextSecondary</item>
    </style>

    <style name="Labels.Body.Normal.Hyperlink">
        <item name="android:textColor">@color/colorTextHyperlink</item>
    </style>

    <style name="Labels.Body.Secondary">
        <item name="android:textSize">@dimen/labels_body_secondary</item>
    </style>
    
    
    <style name="Labels.Navigation">
        <item name="android:fontFamily">@font/open_sans</item>
        
    </style>
    
    <style name="Labels.Navigation.Normal">
        <item name="android:textSize">@dimen/labels_navigation_normal</item>

    </style>

    <style name="Labels.Navigation.Normal.Light">
        <item name="android:textColor">@color/colorTextPrimary</item>
    </style>

    <style name="Labels.Navigation.Normal.Dark">
        <item name="android:textColor">@color/colorTextSecondary</item>
    </style>

    <style name="Labels.Modal">
        <item name="android:textSize">@dimen/labels_modal_summary</item>
        <item name="android:textColor">@color/colorTextSecondary</item>
    </style>

    <style name="Labels.Dialog"/>

    <style name="Labels.Dialog.Custom">
        <item name="android:textSize">@dimen/labels_captions</item>
    </style>

    <style name="Labels.Dialog.Custom.Light">
        <item name="android:textColor">@color/colorTextSecondary</item>
    </style>

    <style name="Labels.List">
        <item name="android:fontFamily">@font/open_sans</item>
    </style>

    <style name="Labels.List.Title">
        <item name="android:textSize">@dimen/labels_lists_titles</item>
        <item name="android:textColor">@color/settings_header_blue_500</item>

    </style>

    <style name="Labels.List.Items">
        <item name="android:textSize">@dimen/labels_lists_items</item>
        <item name="android:textColor">@color/colorTextSecondary</item>

    </style>

    <style name="Labels.List.Items.Light">
        <item name="android:textColor">@color/colorTextPrimary</item>

    </style>

    <style name="Labels.List.Details">
        <item name="android:textSize">@dimen/labels_lists_details</item>
        <item name="android:textColor">@color/grey_800</item>

    </style>

    <style name="Buttons">
        <item name="android:fontFamily">@font/open_sans_light</item>
        <item name="android:layout_margin">@dimen/button_margin</item>
        <item name="android:paddingStart">@dimen/button_padding_start</item>
        <item name="android:paddingEnd">@dimen/button_padding_end</item>

    </style>

    <style name="Buttons.Flat" parent="Widget.AppCompat.Button"/>

    <style name="Buttons.Flat.Light">
        <item name="android:textColor">@color/colorTextPrimary</item>
        <item name="android:textAllCaps">false</item>

    </style>


    <style name="Buttons.Flat.Dark">
        <item name="android:textColor">@color/colorTextSecondary</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Buttons.Flat.Link">
        <item name="android:textColor">@color/colorTextHyperlink</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Buttons.Outline" parent="Widget.AppCompat.Button"/>

    <style name="Buttons.Outline.Light">
        <item name="android:textColor">@color/colorTextPrimary</item>
        <item name="android:background">@drawable/button_rounded_hollow_light</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Buttons.Outline.Dark">
        <item name="android:textColor">@color/colorTextSecondary</item>
        <item name="android:background">@drawable/button_rounded_hollow_dark</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Buttons.Raised" parent="Widget.AppCompat.Button"/>
    <style name="Buttons.Raised.Light">
        <item name="android:textColor">@color/colorTextPrimary</item>
        <item name="android:background">@drawable/button_rounded_filled_light</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Buttons.Raised.Dark">
        <item name="android:textColor">@color/colorTextSecondary</item>
        <item name="android:background">@drawable/button_rounded_filled_dark</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Buttons.Raised.Grey">
        <item name="android:textColor">@color/colorPrimaryNormal</item>
        <item name="android:background">@drawable/button_rounded_filled_grey</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Buttons.Raised.Light.Ghost">
        <item name="android:textColor">@color/colorTextPrimary</item>
        <item name="android:background">@null</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Buttons.Raised.Dark.Ghost">
        <item name="android:textColor">@color/colorTextSecondary</item>
        <item name="android:background">@null</item>
        <item name="android:textAllCaps">false</item>
    </style>


    <style name="Widget.App.Backdrop" parent="">
        <item name="android:background">@color/colorPrimaryNormal</item>

    </style>

</resources>